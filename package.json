{
	"name": "newfolder",
	"private": true,
	"packageManager": "yarn@3.5.0",
	"scripts": {
		"dev": "tsnd --respawn --transpile-only ./server.ts",
		"prettier:fix": "prettier --write \"**/*.(ts|tsx|json|scss)\"",
		"prettier": "prettier src --check",
		"lint": "eslint src",
		"lint:fix": "yarn lint src --fix",
		"format": "yarn prettier:fix && yarn lint:fix"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.62",
		"@swc/core": "^1.3.56",
		"@types/react": "^18.2.6",
		"@typescript-eslint/eslint-plugin": "latest",
		"@typescript-eslint/parser": "latest",
		"chokidar": "^3.5.3",
		"compression-webpack-plugin": "^10.0.0",
		"css-loader": "^6.7.3",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"html-webpack-plugin": "^5.5.1",
		"mini-css-extract-plugin": "^2.7.5",
		"prettier": "^2.8.8",
		"sass": "^1.62.1",
		"sass-loader": "^13.2.2",
		"style-loader": "^3.3.2",
		"swc-loader": "^0.2.3",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.0.4",
		"webpack": "^5.82.0",
		"webpack-cli": "^5.1.0",
		"webpack-dev-server": "^4.14.0"
	},
	"dependencies": {
		"mathjs": "^11.8.0"
	}
}
